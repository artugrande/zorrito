{
  "name": "zorrito-age-verification-contracts",
  "description": "Age verification contracts for Zorrito Finance using Self Protocol",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf cache out",
    "build": "forge build",
    "test": "forge test",
    "deploy:mainnet": "forge script script/DeployProofOfHuman.s.sol:DeployProofOfHuman --rpc-url celo --broadcast --verify",
    "deploy:testnet": "forge script script/DeployProofOfHuman.s.sol:DeployProofOfHuman --rpc-url celo-sepolia --broadcast --verify"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.3.0",
    "@selfxyz/contracts": "^1.2.3"
  }
}

